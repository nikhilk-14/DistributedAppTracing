{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "C:\\home\\site\\wwwroot\\Logs\\tracing-3-{Date}.txt" }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "ApplicationInsights",
                "Args": {
                  "telemetryConverter": "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "ApplicationInsights": {
    "InstrumentationKey": "184375a9-0ab1-4058-8a5f-3fbfee50cdf6"
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "SQLConnectionString": "Server=tcp:test-poc-db.database.windows.net,1433;Initial Catalog=test-db;Persist Security Info=False;User ID=testadmin;Password=Pass@123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  }
}