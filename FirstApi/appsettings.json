{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": { "pathFormat": "C:\\home\\site\\wwwroot\\Logs\\tracing-1-{Date}.txt" }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "ApplicationInsights",
                "Args": {
                  "telemetryConverter": "Serilog.Sinks.ApplicationInsights.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "ApplicationInsights": {
    "InstrumentationKey": "184375a9-0ab1-4058-8a5f-3fbfee50cdf6"
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "SecondApi": "https://tracingsecondapi.azurewebsites.net",
    "ThirdApi": "https://tracingthirdapi.azurewebsites.net",
    "ServiceBusConnection": "Endpoint=sb://distributed-tracing-demo.servicebus.windows.net/;SharedAccessKeyName=ConnectionPolicy;SharedAccessKey=I0FIgQzqKMJEbn1JShl9FTuTEay0tLcNKmXA6p3ZFQw=;EntityPath=input-queue"
  }
}