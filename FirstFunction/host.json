{
  "version": "2.0",
  "extensions": {
    "logLevel": {
      "Host.Triggers.DurableTask": "Information",
      "Function": "Trace",
      "default": "Trace",
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true
        },
        "httpAutoCollectionOptions": {
          "enableW3CDistributedTracing": true
        }
      }
    },
    "durableTask": {
      "hubName": "FunctionHub",
      "tracing": {
        "distributedTracingEnabled": true,
        "distributedTracingProtocol": "W3CTraceContext"
      }
    }
  }
}